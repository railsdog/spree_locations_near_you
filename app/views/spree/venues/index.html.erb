<div class="venue-search">
  <h1>where to buy gather & gown styles</h1>
<div class="row">
  <div class="search-control">
    <div class="col-md-6">
      <form id="searchVenues">
        <input type="text" name="zipcode">
        <button class="venue-search-btn" type="submit"><i class="fa fa-search"></i></button>
      </form>
    </div> <!-- col-md-7 -->
    <div class="col-md-6">
      <div class="rank-checkboxes">
      <%= render partial: 'tier_alert' %>

        <h6 id="storeTiers">STORE TIERS <i class="fa fa-info"></i></h6>
        <% venue_ranks.each do |rank| %>
          <label class="checked">
            <i class="fa fa-check"></i>
            <input class="rank" type="checkbox" name="rank[<%= rank %>]" value="<%= rank %>"/>
          </label><span><%= rank.capitalize %></span>
        <% end %>
      </div> <!-- rank-checkboxes -->
    </div> <!-- col-md-5 -->
  </div> <!-- .search-control -->
</div> <!-- row -->
<div class="row">
  <div class="col-md-8">
    <%= render partial: "map" %>
  </div>
  <div class="col-md-4">
    <%= render partial: "venues_list" %>
  </div>
</div>

